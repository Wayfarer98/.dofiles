# Project navigation aliases
# exa aliases
alias ll='exa -lbF --git --icons'
alias la='exa -lbhHigmuSa --time-style=long-iso --git --color-scale --icons'
alias lx='exa -lbhHigmuSa@ --time-style=long-iso --git --color-scale --icons'
alias llt='exa -l --git --tree --icons'
alias lt='exa --tree --level=2 --icons'
## Sorts
alias llm='exa -lbGF --git --sort=modified --icons'
alias lld='exa -lbhHFGmuSa --group-directories-first --icons'


# Tmux aliases
alias tm='tmux new-session -A -s main'
alias ta='tmux attach'
alias tas='tmux attach -t'
alias tn='tmux new -d -s'
alias tls='tmux ls'
alias tka='tmux kill-session -a'
alias tks='tmux kill-session -t'
